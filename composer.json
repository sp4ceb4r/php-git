{
    "name": "sp4ceb4r/php-git",
    "description": "PHP shell command processing.",
    "license": "MIT",

    "authors": [
        {
            "name": "Jacob Schofield"
        }
    ],

    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/sp4ceb4r/shell-command"
        }
    ],

    "require": {
        "vlucas/phpdotenv": "^2.2",
        "symfony/filesystem": "^3.0",
        "sp4ceb4r/shell-command": "dev-master"
    },

    "require-dev": {
        "phpunit/phpunit": "^5.1"
    },

    "autoload": {
        "psr-4": {
            "Git\\": "src/Git"
        }
    },

    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
